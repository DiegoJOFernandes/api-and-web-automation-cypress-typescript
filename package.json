  "repository": {
    "type": "git",
    "url": "https://github.com/DiegoJOFernandes/api-and-web-automation-cypress-typescript.git"
  },
  "homepage": "https://github.com/DiegoJOFernandes/api-and-web-automation-cypress-typescript#readme",
  "devDependencies": {
    "@faker-js/faker": "^9.8.0",
    "@types/node": "^24.0.3",
    "cypress": "^14.5.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "cypress-plugin-api": "^2.11.2",
    "dotenv": "^16.4.7"
  }
