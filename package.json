{
  "name": "api-and-web-automation-cypress-typescript",
  "version": "1.0.0",
  "description": "Automação de testes de API e WEB para o ServeRest utilizando Cypress + TypeScript.",
  "main": "index.js",
  "scripts": {
    "test:web": "npx cypress run -C cypress.config.ts",
    "test:api": "npx cypress run -C api.config.ts",
    "test:open:web": "npx cypress open -C cypress.config.ts",
    "test:open:api": "npx cypress open -C api.config.ts"
  },
  "keywords": [
    "cypress",
    "automation",
    "testing",
    "api",
    "web",
    "typescript",
    "qa"
  ],
